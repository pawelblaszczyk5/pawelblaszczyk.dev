{
	"$schema": "https://unpkg.com/syncpack@12.3.0/dist/schema.json",
	"lintFormatting": false,
	"indent": "\t",
	"customTypes": {
		"pnpmPeerDependencyRulesAllowedVersions": {
			"path": "pnpm.peerDependencyRules.allowedVersions",
			"strategy": "versionsByName"
		},
		"pnpmOverrides": {
			"path": "pnpm.overrides",
			"strategy": "versionsByName"
		}
	},
	"dependencyTypes": [
		"dev",
		"local",
		"overrides",
		"peer",
		"pnpmOverrides",
		"prod",
		"resolutions",
		"pnpmPeerDependencyRulesAllowedVersions",
		"pnpmOverrides"
	],
	"versionGroups": [
		{
			"label": "Ignore workspace protocol packages",
			"dependencies": [
				"@pawelblaszczyk.dev/**"
			],
			"dependencyTypes": [
				"dev",
				"prod"
			],
			"isIgnored": true
		},
		{
			"dependencies": [
				"@types/**"
			],
			"dependencyTypes": [
				"!dev",
				"!pnpmPeerDependencyRulesAllowedVersions",
				"!pnpmOverrides"
			],
			"isBanned": true,
			"label": "@types packages should only be under devDependencies"
		},
		{
			"dependencies": [
				"react",
				"react-dom"
			],
			"snapTo": [
				"@pawelblaszczyk.dev/website"
			],
			"label": "React core packages should be in-sync"
		}
	],
	"semverGroups": [
		{
			"label": "Ignore workspace protocol packages",
			"dependencies": [
				"@pawelblaszczyk.dev/**"
			],
			"isIgnored": true
		},
		{
			"range": "~",
			"dependencyTypes": [
				"prod",
				"resolutions",
				"overrides",
				"pnpmOverrides",
				"pnpmPeerDependencyRulesAllowedVersions",
				"pnpmOverrides",
				"local"
			],
			"dependencies": [
				"**"
			],
			"packages": [
				"**"
			]
		},
		{
			"range": "^",
			"dependencyTypes": [
				"dev"
			],
			"dependencies": [
				"**"
			],
			"packages": [
				"**"
			]
		}
	]
}
