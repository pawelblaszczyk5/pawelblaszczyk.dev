{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"ignorePaths": ["pnpm-lock.yaml", "**.svg", ".git/", ".gitignore", ".vscode/", "Dockerfile", "drizzle/"],
	"dictionaries": ["lorem-ipsum"],
	"useGitignore": true,
	"words": [
		"buildable",
		"buildinfo",
		"codeowner",
		"corepack",
		"flyctl",
		"focusable",
		"libsql",
		"linebreak",
		"lintable",
		"pawelblaszczyk",
		"plusplus",
		"positionals",
		"signup",
		"superfly",
		"syncpack",
		"taze",
		"textareas",
		"tokenami",
		"topo",
		"tsbuildinfo",
		"turso",
		"typecheck",
		"typedefs",
		"valibot"
	],
	"enableGlobDot": true,
	"enableFiletypes": ["dockerfile", "github-actions-workflow"]
}
